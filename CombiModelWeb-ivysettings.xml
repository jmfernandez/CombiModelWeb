<?xml version="1.0" encoding="UTF-8"?>

<ivysettings>
	<settings defaultResolver="chain"/>
	<caches defaultCacheDir="${CombiModel.dir}/ivy-cache"/>
	<resolvers>
		<chain name="chain">
			<ibiblio name="central" m2compatible="true"/>
			<ibiblio name="gwtupload" m2compatible="true" root="http://gwtupload.googlecode.com/svn/mavenrepo/"/>
			<ibiblio name="gwtchismes" m2compatible="true" root="http://gwtchismes.googlecode.com/svn/mavenrepo/"/>
			<ibiblio name="timepedia" m2compatible="true" root="http://timefire-repository.googlecode.com/svn/mavenrepo/"/>
			<ibiblio name="seasar" m2compatible="true" root="http://maven.seasar.org/maven2"/>
			<ibiblio name="gquery-plugins" m2compatible="true" root="http://gwtquery-plugins.googlecode.com/svn/mavenrepo"/>
		</chain>
	</resolvers>
</ivysettings>
